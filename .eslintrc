{
  "rules": {
    "no-console": 2,
    "no-alert": 2,
    "no-debugger": 2,
    "comma-dangle": [2, "never"],
    "quotes": [2, "single", "avoid-escape"],
    "no-unused-vars": [2, {"args": "after-used", "argsIgnorePattern": "^_"}],
    "no-empty": 2,
    "no-cond-assign": [2, "always"],
    "no-constant-condition": 2,
    "no-dupe-keys": 2,
    "no-extra-parens": 2,
    "no-extra-semi": 2,
    "no-inner-declarations": 2,
    "no-irregular-whitespace": 2,
    "no-negated-in-lhs": 2,
    "no-sparse-arrays": 2,
    "no-unexpected-multiline": 2,
    "no-unreachable": 2,
    "valid-typeof": 2,
    "block-scoped-var": 2,
    "consistent-return": 2,
    "curly": 2,
    "dot-location": [2, "property"],
    "eqeqeq": [2, "smart"],
    "no-else-return": 2,
    "no-implicit-coercion": [2, {"boolean": true, "number": true, "string": true}],
    "no-lone-blocks": 2,
    "no-magic-numbers": [2, {"ignore": [8080]}],
    "no-loop-func": 2,
    "no-multi-spaces": 2,
    "no-multi-str": 2,
    "no-new-wrappers": 2,
    "no-new": 2,
    "no-throw-literal": 2,
    "no-redeclare": 2,
    "no-useless-call": 2,
    "no-useless-concat": 2,
    "vars-on-top": 2,
    "yoda": 2,
    "no-catch-shadow": 2,
    "no-shadow-restricted-names": 2,
    "no-shadow": 2,
    "no-use-before-define": 2,
    "array-bracket-spacing": [2, "never"],
    "block-spacing": 2,
    "brace-style": [2, "1tbs", { "allowSingleLine": true }],
    "camelcase": [2, {"properties": "always"}],
    "comma-spacing": [2, {"before": false, "after": true}],
    "comma-style": [2, "last"],
    "computed-property-spacing": [2, "never"],
    "consistent-this": [2, "self"],
    "eol-last": 2,
    "indent": [2, 2, {"SwitchCase": 1}],
    "key-spacing": [2, {"beforeColon": false, "afterColon": true}],
    "new-cap": 2,
    "new-parens": 2,
    "newline-after-var": [2, "always"],
    "no-lonely-if": 2,
    "no-multiple-empty-lines": [2, {"max": 1, "maxEOF": 1}],
    "no-negated-condition": 2,
    "no-nested-ternary": 2,
    "no-spaced-func": 2,
    "no-trailing-spaces": 2,
    "no-unneeded-ternary": 2,
    "object-curly-spacing": [2, "never"],
    "operator-linebreak": [2, "before"],
    "padded-blocks": [2, "never"],
    "quote-props": [2, "consistent-as-needed"],
    "semi-spacing": 2,
    "space-after-keywords": 2,
    "space-before-blocks": 2,
    "space-before-function-paren": [2, { "anonymous": "always", "named": "never" }],
    "space-before-keywords": [2, "always"],
    "space-in-parens": [2, "never"],
    "space-infix-ops": 2,
    "space-return-throw-case": 2,
    "space-unary-ops": 2,
    "semi": [2, "always"],
    "backbone/initialize-on-top": [2, {
      "View": ["width", "tagName", "className", "attributes", "template", "alwaysSuppressHighlightErrors", "subViews", "events", "constructor"],
      "Model": ["defaults"],
      "Collection": ["model"]
    }],
    "backbone/events-on-top": [2, ["tagName", "className", "attributes", "template", "alwaysSuppressHighlightErrors", "subViews"]]
  },
  "env": {
    "browser": true,
    "jquery": true,
    "jasmine": true
  },
  "plugins": [
    "backbone"
  ],
  "globals": {
    "Backbone": true,
    "_": true,
    "requirejs": true,
    "require": true,  
    "define": true,
    "ejs": true,
    "module": true,
    "process": true
  },
  "settings": {
    "backbone": {
      "Collection": [
      ],
      "Model": [
      ],
      "View": [
      ]
    }
  },
  "extends": "eslint:recommended"
}